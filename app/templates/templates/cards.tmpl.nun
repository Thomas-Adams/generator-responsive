<div class="card">
    <div class="card-image">
        <img src="{{ card.image }}" alt="{{ card.alt }}">
    </div>
    <div class="card-header">{{ card.header }}</div>
    <div class="card-copy">
        {% for par in card.paragraphs %} 
            <p>{{ card.par }}</p>
        {% endfor %}
    </div>
</div>